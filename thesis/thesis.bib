@Article{     shabala_size_2013,
  title     = {{SIZE} {DEPENDENCE} {OF} {THE}
          {RADIO}-{LUMINOSITY}-{MECHANICAL}-{POWER} {CORRELATION}
          {IN} {RADIO} {GALAXIES}},
  volume    = {769},
  issn      = {0004-637X, 1538-4357},
  url       = {http://stacks.iop.org/0004-637X/769/i=2/a=129?key=crossref.8fb6432b83281c98a5f254e4c32cc1d0},
  doi       = {10.1088/0004-637X/769/2/129},
  abstract  = {We examine the relationship between source radio
          luminosity and kinetic power in active galactic nucleus
          jets. We show that neglecting various loss processes can
          introduce a systematic bias in the jet powers inferred from
          radio luminosities for a sample of radio galaxies. This
          bias can be corrected for by considering source size as
          well as radio luminosity; effectively the source size acts
          as a proxy for source age. Based on a sample of
          Fanaroff{\textendash}Riley Type II radio sources with jet
          powers derived from the measured hotspot parameters, we
          empirically determine a new expression for jet power that
          accounts for the source size, (Qjet/1036 W) =
          1.5+-10..88(L151/1027 W Hz-1)0.8(1 +
          z)1.0(D/kpc)0.58{\textpm}0.17, where D is source size and
          L151 the 151 MHz radio luminosity. By comparing a
          flux-limited and volume-limited sample, we show that any
          derived radio-luminosity{\textendash}jet-power relation
          depends sensitively on sample properties, in particular the
          source size distribution and the
          size{\textendash}luminosity correlation inherent in the
          sample. Such bias will affect the accuracy of the kinetic
          luminosity function derived from lobe radio luminosities
          and should be treated with caution.},
  language  = {en},
  number    = {2},
  urldate   = {2020-04-02},
  journal   = {The Astrophysical Journal},
  author    = {Shabala, S. S. and Godfrey, L. E. H.},
  month     = may,
  year      = {2013},
  pages     = {129},
  file      = {Shabala and Godfrey - 2013 - SIZE DEPENDENCE OF THE
          RADIO-LUMINOSITY-MECHANICAL.pdf:C\:\\Users\\matthew\\Zotero\\storage\\SBSEHWK6\\Shabala
          and Godfrey - 2013 - SIZE DEPENDENCE OF THE
          RADIO-LUMINOSITY-MECHANICAL.pdf:application/pdf}
}

