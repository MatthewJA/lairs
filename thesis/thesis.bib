  file      = {Full Text
          PDF:C\:\\Users\\matthew\\Zotero\\storage\\ZU2LHAGG\\Panessa
          et al. - 2019 - The origin of radio emission from
          radio-quiet acti.pdf:application/pdf}
}

@Article{     panwar_alignment_2020,
  title     = {Alignment of {Radio} {Galaxy} {Axes} using {FIRST}
          {Catalogue}},
  issn      = {0035-8711, 1365-2966},
  url       = {http://arxiv.org/abs/2006.09779},
  doi       = {10.1093/mnras/staa2975},
  abstract  = {We study the alignment of radio galaxies axes using the
          FIRST catalogue. we impose several cuts in order to select
          the candidates which are most likely to be free of
          systematic bias. In our study we primarily focus on testing
          for alignment among sources within a certain angular
          separation from one another since for most sources redshift
          information is not available. We find a very significant
          effect for angular distances less than 1 degrees. The
          distance scale of alignment is found to be roughly 28 Mpc,
          in agreement with earlier estimates, assuming that these
          sources are dominantly at redshift of 0.8. However we are
          not able to entirely rule out the possibility of systematic
          bias in data. We also perform a full three dimensional
          analysis using a smaller data sample for which redshift
          information is available. In this case we only find a very
          weak signal at much larger distances.},
  urldate   = {2021-01-24},
