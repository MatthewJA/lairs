  pages     = {A6},
  file      = {Full Text
          PDF:C\:\\Users\\matthew\\Zotero\\storage\\ZAQU7369\\Smol{\v
          c}i{\'c} et al. - 2017 - The VLA-COSMOS 3 GHz Large Project
          Cosmic evoluti.pdf:application/pdf}
}

