@Article{     singh_context_2018,
  title     = {Context {Mover}'s {Distance} \& {Barycenters}: {Optimal}
          {Transport} of {Contexts} for {Building} {Representations}},
  shorttitle    = {Context {Mover}'s {Distance} \& {Barycenters}},
  url       = {https://arxiv.org/abs/1808.09663v6},
  abstract  = {We present a framework for building unsupervised
          representations of entities and their compositions, where
          each entity is viewed as a probability distribution rather
          than a vector embedding. In particular, this distribution
          is supported over the contexts which co-occur with the
          entity and are embedded in a suitable low-dimensional
          space. This enables us to consider representation learning
          from the perspective of Optimal Transport and take
          advantage of its tools such as Wasserstein distance and
          barycenters. We elaborate how the method can be applied for
          obtaining unsupervised representations of text and
          illustrate the performance (quantitatively as well as
          qualitatively) on tasks such as measuring sentence
          similarity, word entailment and similarity, where we
          empirically observe significant gains (e.g., 4.1\% relative
          improvement over Sent2vec, GenSen). The key benefits of the
          proposed approach include: (a) capturing uncertainty and
          polysemy via modeling the entities as distributions, (b)
          utilizing the underlying geometry of the particular task
          (with the ground cost), (c) simultaneously providing
          interpretability with the notion of optimal transport
          between contexts and (d) easy applicability on top of
          existing point embedding methods. The code, as well as
          prebuilt histograms, are available under
          https://github.com/context-mover/.},
  language  = {en},
  urldate   = {2020-06-16},
  author    = {Singh, Sidak Pal and Hug, Andreas and Dieuleveut, Aymeric
          and Jaggi, Martin},
  month     = aug,
  year      = {2018},
  file      = {Full Text
          PDF:C\:\\Users\\matthew\\Zotero\\storage\\JFU9WIZE\\Singh
          et al. - 2018 - Context Mover's Distance & Barycenters
          Optimal
          Tr.pdf:application/pdf;Snapshot:C\:\\Users\\matthew\\Zotero\\storage\\8EU2IKKC\\1808.html:text/html}
}

@Article{     smolcic_vla-cosmos_2017,
