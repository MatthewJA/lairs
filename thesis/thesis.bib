@Article{     georgiou_metrics_2009,
  title     = {Metrics for {Power} {Spectra}: {An} {Axiomatic}
          {Approach}},
  volume    = {57},
  issn      = {1053-587X, 1941-0476},
  shorttitle    = {Metrics for {Power} {Spectra}},
  url       = {http://ieeexplore.ieee.org/document/4689383/},
  doi       = {10.1109/TSP.2008.2010009},
  abstract  = {We present an axiomatic framework for seeking distances
          between power spectral density functions. The axioms
          require that the sought metric respects the effects of
          additive and multiplicative noise in reducing our ability
          to discriminate spectra, as well as they require continuity
          of statistical quantities with respect to perturbations
          measured in the metric. We then present a particular metric
          which abides by these requirements. The metric is based on
          the Monge-Kantorovich transportation problem and is
          contrasted with an earlier Riemannian metric based on the
          minimum-variance prediction geometry of the underlying
          time-series. It is also being compared with the more
          traditional Itakura-Saito distance measure, as well as the
          aforementioned prediction metric, on two representative
          examples.},
  language  = {en},
  number    = {3},
  urldate   = {2020-03-04},
  journal   = {IEEE Transactions on Signal Processing},
  author    = {Georgiou, T.T. and Karlsson, J. and Takyar, M.S.},
  month     = mar,
  year      = {2009},
  pages     = {859--867},
  file      = {Georgiou et al. - 2009 - Metrics for Power Spectra An
          Axiomatic
          Approach.pdf:C\:\\Users\\matthew\\Zotero\\storage\\PJ8DY5EF\\Georgiou
          et al. - 2009 - Metrics for Power Spectra An Axiomatic
          Approach.pdf:application/pdf}
}

@Article{     gheller_deep_2018,
  title     = {Deep learning based detection of cosmological diffuse
          radio sources},
  volume    = {480},
  issn      = {0035-8711},
  url       = {http://adsabs.harvard.edu/abs/2018MNRAS.480.3749G},
  doi       = {10.1093/mnras/sty2102},
  abstract  = {In this paper we introduce a reliable, fully automated and
          fast algorithm to detect extended extragalactic radio
          sources (cluster of galaxies, filaments) in existing and
          forthcoming surveys (like LOFAR and SKA). The proposed
          solution is based on the adoption of a Deep Learning
          approach, more specifically a Convolutional Neural Network,
          that proved to perform outstandingly in the processing,
          recognition and classification of images. The challenge, in
          the case of radio interferometric data, is the presence of
          noise and the lack of a sufficiently large number of
          labelled images for the training. We have specifically
