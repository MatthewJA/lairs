@Article{     wong_determining_2016,
  title     = {Determining the radio active galactic nuclei contribution
          to the radio-far-infrared correlation using the black hole
          {Fundamental} {Plane} relation},
  volume    = {460},
  issn      = {0035-8711},
  url       = {http://adsabs.harvard.edu/abs/2016MNRAS.460.1588W},
  doi       = {10.1093/mnras/stw957},
  abstract  = {We investigate the 1.4-GHz radio properties of 92 nearby
          (z {\textless} 0.05) ultra-hard X-ray selected active
          galactic nuclei (AGNs) from the Swift Burst Alert Telescope
          (BAT) sample. Through the ultra-hard X-ray selection, we
          minimize the biases against obscured or Compton-thick AGNs
          as well as confusion with emission derived from star
          formation that typically affect AGN samples selected from
          the ultraviolet, optical and infrared wavelengths. We find
          that all the objects in our sample of nearby, ultra-hard
          X-ray selected AGNs are radio quiet; 83 per cent of the
          objects are classed as high-excitation galaxies and 17 per
          cent as low-excitation galaxies. While these low-z BAT
          sources follow the radio-far-infrared correlation in a
          similar fashion to star-forming galaxies, our analysis
          finds that there is still significant AGN contribution in
          the observed radio emission from these radio-quiet AGNs. In
          fact, the majority of our BAT sample occupy the same
          X-ray-radio Fundamental Plane as has been observed in other
          samples, which include radio-loud AGNs - evidence that the
          observed radio emission (albeit weak) is connected to the
          AGN accretion mechanism, rather than star formation.},
  urldate   = {2020-07-14},
  journal   = {Monthly Notices of the Royal Astronomical Society},
  author    = {Wong, O. Ivy and Koss, M. J. and Schawinski, K. and
          Kapi{\'n}ska, A. D. and Lamperti, I. and Oh, K. and Ricci,
          C. and Berney, S. and Trakhtenbrot, B.},
  month     = aug,
  year      = {2016},
  keywords  = {galaxies: active, radio continuum: galaxies, galaxies:
          evolution, galaxies: Seyfert, X-rays: galaxies},
  pages     = {1588--1597},
  file      = {Full Text
          PDF:C\:\\Users\\matthew\\Zotero\\storage\\IEJ6W746\\Wong et
          al. - 2016 - Determining the radio active galactic nuclei
          contr.pdf:application/pdf}
}

