@Article{     malarecki_large_2011,
  title     = {Large scale structure, the {Warm}-{Hot} {IGM} and giant
          radio galaxies},
  url       = {http://adsabs.harvard.edu/abs/2011atnf.prop.4392M},
  abstract  = {This proposal is a continuation of our previous
          observations (C2340) to complete the sample of giant radio
          galaxies described below. One of the key challenges today
          is to account for nearly half of the baryons in the
          present-day universe that are missing. These missing
          baryons are thought to pervade the moderate overdensity
          filaments (with fractional overdensity {\textasciitilde}10
          to 100) of the large-scale structure as a Warm-Hot
          intergalactic medium (WHIM) of 10{\textasciicircum}5 to
          10{\textasciicircum}7 K gas. The state of the gas is a
          consequence of the hydrodynamic infall heating and, more
          importantly, the astrophysical feedback on the environment
          from galaxy formation and evolution. We propose to use a
          sample of giant radio galaxies embedded in the filaments,
          with radio jets that interact with the gas beyond and
          outside of their host galaxy, as a probe of the WHIM.
          Modelling the jet-WHIM interaction and the evolution of the
          radio synchrotron plasma lobes in the gas and gravitational
          field of the surrounding large-scale structure constitutes
          a novel method of constraining the physical parameters and
          cosmological evolution of the WHIM gas, and thereby
          assessing the importance of feedback from galaxy evolution
          on the state of this gas.},
  urldate   = {2020-07-14},
  journal   = {ATNF Proposal},
  author    = {Malarecki, Jurek and Staveley-Smith, Lister and
          Subrahmanyan, Ravi and Saripalli, Lakshmi and Jones,
          Heath},
  month     = oct,
  year      = {2011},
  keywords  = {active galactic nuclei: quasars, ATCA},
  pages     = {C2340}
}

@Article{     malefahlo_optically-selected_2019,
  title     = {The optically-selected 1.4-{GHz} quasar luminosity
          function below 1 {mJy}},
  url       = {http://arxiv.org/abs/1908.05316},
  abstract  = {The dominant source of radio emission from quasars has
          been debated for decades. We present the radio luminosity
          function (RLF) of optically-selected quasars below 1 mJy,
          constructed by applying a Bayesian-fitting stacking
          technique to objects below the nominal radio flux-density
          limit. We test the technique using simulated data,
          confirming that we can reconstruct the RLF over three
          orders of magnitude below the typical 5$\sigma$ threshold,
          although this is dependent on the number density of the
          population in question. We apply our method to 1.4-GHz
          flux-densities from the Faint Images of the Radio Sky at
          Twenty-cm (FIRST) survey, extracted at the positions of
          optical quasars from the Sloan Digital Sky Survey (SDSS
          DR7) over seven redshift bins. The lowest redshift bin (0.2
          {\textless} z {\textless} 0.45) constitutes a
          volume-limited sampled defined by Mi {\textless} -23 and we
          measure the optically-selected quasar RLF over two orders
          of magnitude below the FIRST detection threshold of 1 mJy.
          We find that our measured RLF is in agreement with deeper
          data from the literature. We find that the radio luminosity
          function for the radio-loud quasars flattens around
          log10[L1.4/W Hz-1] ? 25, where radio-quiet quasars start to
          emerge. The radio luminosity were radio-quiet quasars
          emerge coincides with the luminosity where star-forming
          galaxies start to dominate the radio source counts. This
          implies that there could be a significant contribution from
          star formation in the host galaxies, but additional data is
          required to investigate this further. The higher-redshift
          bins show similar behaviour (i.e. RLF shape) as for the
          lowest-z bin, at least up to z = 2.15, implying that the
          same physical process may be responsible.},
  language  = {en},
  urldate   = {2019-11-05},
  journal   = {arXiv:1908.05316 [astro-ph]},
  author    = {Malefahlo, Eliab and Santos, Mario G. and Jarvis, Matt J.
          and White, Sarah V. and Zwart, Jonathan T. L.},
  month     = aug,
  year      = {2019},
  note      = {arXiv: 1908.05316},
  keywords  = {Astrophysics - Astrophysics of Galaxies},
  file      = {Malefahlo et al. - 2019 - The optically-selected 1.4-GHz
          quasar luminosity
          f.pdf:C\:\\Users\\matthew\\Zotero\\storage\\HD7BPTSZ\\Malefahlo
          et al. - 2019 - The optically-selected 1.4-GHz quasar
          luminosity f.pdf:application/pdf}
