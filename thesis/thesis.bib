@Article{     george_detection_2012,
  title     = {Detection {Thresholds} and {Bias} {Correction} in
          {Polarized} {Intensity}},
  volume    = {29},
  issn      = {1323-3580, 1448-6083},
  url       = {http://arxiv.org/abs/1106.5362},
  doi       = {10.1071/AS11027},
  abstract  = {Detection thresholds in polarized intensity and
          polarization bias correction are investigated for surveys
          where the polarization information is obtained from RM
          synthesis. Considering unresolved sources with a single
          rotation measure, a detection threshold of \$8
          {\textbackslash}sigma\_\{QU\}\$ applied to the Faraday
          spectrum will retrieve the RM with a false detection rate
          less than \$10{\textasciicircum}\{-4\}\$, but polarized
          intensity is more strongly biased than Ricean statistics
          suggest. For a detection threshold of \$5
          {\textbackslash}sigma\_\{QU\}\$, the false detection rate
          increases to {\textasciitilde}4\%, depending also on
          \${\textbackslash}lambda{\textasciicircum}2\$ coverage and
          the extent of the Faraday spectrum. Non-Gaussian noise in
          Stokes Q and U due to imperfect imaging and calibration can
          be represented by a distribution that is the sum of a
          Gaussian and an exponential. The non-Gaussian wings of the
          noise distribution increase the false detection rate in
          polarized intensity by orders of magnitude. Monte-Carlo
          simulations assuming non-Gaussian noise in Q and U, give
          false detection rates at \$8
          {\textbackslash}sigma\_\{QU\}\$ similar to Ricean false
          detection rates at \$4.9 {\textbackslash}sigma\_\{QU\}\$.},
  number    = {3},
  urldate   = {2019-09-12},
  journal   = {Publications of the Astronomical Society of Australia},
  author    = {George, Samuel J. and Stil, Jeroen M. and Keller, Ben W.},
  year      = {2012},
  note      = {arXiv: 1106.5362},
  keywords  = {Astrophysics - Instrumentation and Methods for
          Astrophysics, Astrophysics - Cosmology and Nongalactic
          Astrophysics},
  pages     = {214--220},
  file      = {arXiv\:1106.5362
          PDF:C\:\\Users\\matthew\\Zotero\\storage\\CE9JYVIH\\George
          et al. - 2012 - Detection Thresholds and Bias Correction in
          Polari.pdf:application/pdf;arXiv.org
