}

@Article{     shore_minimum_1981,
  title     = {Minimum cross-entropy spectral analysis},
  volume    = {29},
  issn      = {0096-3518},
  doi       = {10.1109/TASSP.1981.1163539},
  abstract  = {The principle of minimum cross-entropy (minimum directed
          divergence, minimum discrimination information, minimum
          relative entropy) is summarized, discussed, and applied to
          the classical problem of estimating power spectra given
          values of the autocorrelation function. This new method
          differs from previous methods in its explicit inclusion of
          a prior estimate of the power spectrum, and it reduces to
          maximum entropy spectral analysis as a special case. The
          prior estimate can be viewed as a means of shaping the
          spectral estimator. Cross-entropy minimization yields a
          family of shaped spectral estimators consistent with known
          autocorrelations. Results are derived in two equivalent
          ways: once by minimizing the cross-entropy of underlying
          probability densities, and once by arguments concerning the
          cross-entropy between the input and output of linear
          filters. Several example minimum cross-entropy spectra are
          included.},
  number    = {2},
  journal   = {IEEE Transactions on Acoustics, Speech, and Signal
          Processing},
  author    = {Shore, J.},
  month     = apr,
  year      = {1981},
  keywords  = {Equations, Vector quantization, Entropy, Spectral
          analysis, Autocorrelation, Distortion measurement, Linear
          predictive coding, Measurement standards, Particle
          measurements, Yield estimation},
  pages     = {230--237},
  file      = {a064183.pdf:C\:\\Users\\matthew\\Zotero\\storage\\X2LTI39W\\a064183.pdf:application/pdf;IEEE
          Xplore Abstract
          Record:C\:\\Users\\matthew\\Zotero\\storage\\SBXMU3BV\\1163539.html:text/html}
